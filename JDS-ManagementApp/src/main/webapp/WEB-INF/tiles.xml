      		<add-attribute value="/js/app/MenuService.js"/>
      		<add-attribute value="/js/app/MenuController.js"/>
      		<add-attribute value="/js/app/TableService.js"/>
      		<add-attribute value="/js/app/TableController.js"/>
      		<add-attribute value="/js/app/StaffService.js"/>
      		<add-attribute value="/js/app/StaffController.js"/>
      		<add-attribute value="/js/app/ProfileController.js"/>
        </put-list-attribute>
        
        <put-list-attribute name="fonts">
            <add-attribute value="/fonts.googleapis.com/css?family=Roboto+Condensed:400,300,300italic,400italic,700,700italic"/>
        </put-list-attribute>
        
        
